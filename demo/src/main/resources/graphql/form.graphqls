# 用户ID
input UserID {
    username: String
    email: String
}

# 登录凭据
input LoginValue {
    # 密码/code
    value: String!
    # 验证码问题key
    captchaKey: String
    # 验证码回答val
    captchaVal: String
}

# 查询语句
input FormQuery {
    limit: Int!
    skip: Int
    page: Int
    sort: [String!]
    group: [String!]
    keyJson: String
}
